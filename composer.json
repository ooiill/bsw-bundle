{
  "name": "ooiill/bsw-bundle",
  "type": "symfony-bundle",
  "description": "A web infrastructure bundle for Symfony",
  "keywords": [
    "bsw",
    "web",
    "symfony"
  ],
  "homepage": "https://github.com/ooiill/bsw-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "ooiill",
      "email": "jiangxilee@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "ext-bcmath": "*",
    "ext-ctype": "*",
    "ext-curl": "*",
    "ext-dom": "*",
    "ext-fileinfo": "*",
    "ext-gd": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-mcrypt": "*",
    "ext-openssl": "*",
    "ext-pdo": "*",
    "ext-zip": "*",
    "ext-redis": "*",
    "ext-zlib": "*",
    "ext-libxml": "*",
    "ext-simplexml": "*",
    "alibabacloud/client": "^1.0",
    "aliyuncs/oss-sdk-php": "^2.3",
    "geoip2/geoip2": "~2.0",
    "aws/aws-sdk-php": "^3.69",
    "bshaffer/oauth2-server-php": "^1.10",
    "endroid/qr-code": "^3.4",
    "gregwar/captcha": "^1.1",
    "hashids/hashids": "^3.0",
    "intervention/image": "^2.4",
    "irazasyed/telegram-bot-sdk": "^3.0",
    "lcobucci/jwt": "^3.2",
    "league/oauth2-server": "^7.3",
    "nesbot/carbon": "^2.6",
    "w7corp/easywechat": "*",
    "paragonie/constant_time_encoding": "^2.2",
    "phpoffice/phpspreadsheet": "^1.6",
    "phpseclib/phpseclib": "^2.0",
    "predis/predis": "^1.1",
    "qcloudsms/qcloudsms_php": "^0.1.4",
    "qcloud/cos-sdk-v5": "2.*",
    "respect/validation": "^1.1",
    "spomky-labs/otphp": "^9.1",
    "kriswallsmith/buzz": "^1.0",
    "mailgun/mailgun-php": "^3.0",
    "nyholm/psr7": "^1.2",
    "yansongda/pay": "^2.9",
    "mexitek/phpcolors": "^0.4.0",
    "envms/fluentpdo": "^2.1",
    "markrogoyski/math-php": "1.*",
    "erusev/parsedown": "^1.7",
    "symfony/profiler-pack": "^1.0",
    "ocramius/proxy-manager": "*",
    "phpmailer/phpmailer": "^6.1.6",
    "friendsofsymfony/rest-bundle": "*",
    "knplabs/knp-paginator-bundle": "*",
    "snc/redis-bundle": "*",
    "jms/serializer-bundle": "*",
    "symfony/security-csrf": "*",
    "symfony/web-profiler-bundle": "5.4.*",
    "symfony/serializer": "5.4.*",
    "symfony/webpack-encore-bundle": "^1.12"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "Leon\\BswBundle\\": ""
    }
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.4.*"
    }
  },
  "require-dev": {
    "symfony/maker-bundle": "^1.11"
  }
}
